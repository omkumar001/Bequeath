<% if(ngo.length==0){ %>
  <h2>No NGO in this category</h2>
<%  }  else { %>

  

      <% ngo.forEach(function(f1){ %>

          <div class="card mt-4 ml-7 details ngo_cards show " style="width: 33rem;">
            <!-- <img src="/images/donate_card.png"   style=" width:100%; " alt="">  -->

            <div class="card-body">
            
              <p class="card-text "> 
               
                <h2 class="ngotitle"><%=f1.ngotitle%></h2>
                <br><br>
                
                <p class= "details"></p>

                
                Owner :  <span class="info"> <%=f1.ownerusername  %>  </span> <br>
                NGO Type : <span class="info">  <%=f1.cateogary  %> </span>   <br>
                Registration No. : <span class="info">  <%=f1.ngoregno  %> </span>   <br>
                Address : <span class="info"><%=f1.ngoaddress  %> </span>   <br>
                Current Funds : <span class="info"> â‚¹ <%= f1.currentfunds %> </span> <br>
                Website : <span class="info"><a style="text-decoration: none; color:black" href="<%= f1.websiteurl %>"> <%= f1.websiteurl %>  </a>   </span> <br>
                 <br><br>
                About : - <br><br>
                <%=f1.aboutngo  %>   <br><br>
                <br><br>

                  Registration Certificate :- <br><br>
                 <img src="data:images/uploads/<%=f1.regcertificateimg.contentType%>;base64,
                       <%=f1.regcertificateimg.data.toString('base64')%>"  style=" width:400px; height:300px; " > <br><br>

                <a  style="float: right;" href="/donate/<%=Donoremai%>/<%=f1.ownerusername%>"><button type="button" class="btn btn-outline-primary">Donate  now</button></a><br><br>

              </p>


            </div>
          </div>

<% }); %>
